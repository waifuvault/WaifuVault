{
  "name": "waifu_vault",
  "version": "1.0.0",
  "description": "Temp file hosting for nerds",
  "type": "module",
  "scripts": {
    "lint": "eslint --no-fix --max-warnings 0 .",
    "lint:fix": "eslint --fix .",
    "tsc": "tsc --project tsconfig.json",
    "build": "npm run barrels && node ./buildScripts/cleanDist.mjs && swc src -d dist --config-file .swcrc --copy-files --strip-leading-paths",
    "barrels": "barrels",
    "start_js": "node dist/index.js",
    "start_js:ci": "node dist/index.js -ghAction",
    "runmigration:ci": "node dist/db/runMigration.js -ghAction",
    "start": "node --loader ts-node/esm src/index.ts",
    "start:prod": "cross-env NODE_ENV=production nodemon dist/index.js",
    "update_deps": "npx npm-check-updates",
    "typeorm": "typeorm-ts-node-esm",
    "blankmigration": "typeorm-ts-node-esm migration:create",
    "generatemigration": "typeorm-ts-node-esm migration:generate -d ./src/db/DataSource.ts",
    "runmigration": "typeorm-ts-node-esm migration:run -d ./src/db/DataSource.ts",
    "revertmigration": "typeorm-ts-node-esm migration:revert -d ./src/db/DataSource.ts",
    "prettier": "prettier ./src --check",
    "prettier:fix": "prettier ./src --write",
    "prepare": "husky",
    "docker:compose:build": "docker compose up -d --build"
  },
  "dependencies": {
    "@socket.io/redis-adapter": "^8.3.0",
    "@tsed/ajv": "^8.18.0",
    "@tsed/core": "^8.18.0",
    "@tsed/di": "^8.18.0",
    "@tsed/engines": "^8.18.0",
    "@tsed/exceptions": "^8.18.0",
    "@tsed/hooks": "^8.18.0",
    "@tsed/ioredis": "^8.18.0",
    "@tsed/json-mapper": "^8.18.0",
    "@tsed/logger": "8.0.4",
    "@tsed/logger-file": "8.0.4",
    "@tsed/logger-std": "^8.0.4",
    "@tsed/passport": "^8.18.0",
    "@tsed/platform-cache": "^8.18.0",
    "@tsed/platform-exceptions": "^8.18.0",
    "@tsed/platform-express": "^8.18.0",
    "@tsed/platform-http": "^8.18.0",
    "@tsed/platform-log-middleware": "^8.18.0",
    "@tsed/platform-log-request": "^8.18.0",
    "@tsed/platform-middlewares": "^8.18.0",
    "@tsed/platform-multer": "^8.18.0",
    "@tsed/platform-params": "^8.18.0",
    "@tsed/platform-response-filter": "^8.18.0",
    "@tsed/platform-views": "^8.18.0",
    "@tsed/scalar": "^8.18.0",
    "@tsed/schema": "^8.18.0",
    "@tsed/socketio": "^8.18.0",
    "@tsed/swagger": "^8.18.0",
    "ajv": "^8.17.1",
    "archiver": "^7.0.1",
    "argon2": "^0.44.0",
    "better-sqlite3": "^12.4.1",
    "body-parser": "^2.2.0",
    "builder-pattern": "^2.2.0",
    "compression": "^1.8.1",
    "connect-redis": "^8.1.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cronstrue": "^3.3.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "dotenv-expand": "^12.0.3",
    "dotenv-flow": "^4.1.0",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "ffmpeg-ffprobe-static": "^6.1.2-rc.1",
    "file-type": "^21.0.0",
    "fluent-ffmpeg": "^2.1.3",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.1",
    "is-localhost-ip": "^2.0.0",
    "method-override": "^3.0.0",
    "mime": "^4.1.0",
    "node-schedule": "^2.1.1",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^8.16.3",
    "punycode": "^2.3.1",
    "redis": "^4.7.1",
    "socket.io": "^4.8.1",
    "toad-scheduler": "^3.1.0",
    "typeorm": "^0.3.27",
    "typeorm-rate-limit-store": "^1.1.1",
    "uuid": "^13.0.0",
    "xss": "^1.0.15",
    "xxhash-wasm": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@swc/cli": "0.7.8",
    "@swc/core": "^1.13.5",
    "@tsed/barrels": "^6.6.3",
    "@tsed/cli-plugin-passport": "6.6.3",
    "@types/archiver": "^6.0.3",
    "@types/cache-manager-ioredis": "^2.0.7",
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-session": "^1.18.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/jsdom": "^27.0.0",
    "@types/method-override": "^3.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.8.1",
    "@types/node-schedule": "^2.1.8",
    "@types/passport": "^1.0.17",
    "@types/passport-http": "^0.3.11",
    "@types/passport-local": "^1.0.38",
    "@types/punycode": "^2.1.4",
    "eslint": "^9.38.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-files": "^1.1.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.46.1"
  },
  "main": "index.js",
  "keywords": [],
  "author": "Victoria",
  "license": "MIT",
  "tsed": {
    "packageManager": "npm",
    "convention": "default",
    "architecture": "default"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "tsc-files --noEmit --pretty",
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
}
